//item L job
//function: cumulative
#include<stdio.h>
int he(int n)
{
	int a=1;
	int sum=0;
	while(a<=n)
		sum+=a++;
	//+=çš„æ„æ€:sum=sum+a++;
	//a++æ˜¯å…ˆä½¿ç”¨açš„å€¼ï¼Œç„¶åå†å¯¹aåšåŠ 1å¤„ç†
	//è€Œ++aè¡¨ç¤ºå…ˆå¯¹aåšåŠ 1å¤„ç†ï¼Œå†ä½¿ç”¨açš„å€¼ã€‚a++å’Œ++aéƒ½ç­‰ä»·äºã€a=a+1ã€‘.
	return sum;
}
int main ()
{
	int n;
	scanf("%d",&n);
	printf("%d",he(n));
	return 0;
}
