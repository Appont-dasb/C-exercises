#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>

//ä¸¤ä¸ªæ•°çš„äºŒè¿›åˆ¶ä¸åŒä½çš„ä¸ªæ•°
//åˆ©ç”¨ æŒ‰ä½å¼‚æˆ– ï¼šç›¸åŒçš„ä½0 ï¼Œä¸åŒçš„ä¸º1
//å†æ ¹æ®ä¸Šé¢ï¼šè®¡ç®—1ä¸ªä¸ªæ•°
//å³å¯å†™å‡º
int get_diff_bit(int a,int b)
{
	int count = 0 ;
	int tmp = a ^ b;
	while(tmp)
	{
		tmp = tmp &(tmp-1);//âœ¨
		count ++ ;
	}
	return count ;
	
}
int main()
{
	int a = 0 , b = 0 ;
	int c = 0;
	scanf("%d%d",&a,&b);
	c = get_diff_bit(a,b);
	printf("ä¸åŒä½çš„ä¸ªæ•°ï¼š%d\n",c);
	return 0 ;
}


//åˆ†åˆ«æ‰“å°å‡ºä¸€ä¸ªæ•°è½¬æ¢æˆäºŒè¿›åˆ¶æ•°åçš„å¥‡æ•°ä½å’Œå¶æ•°ä½
void print(int a)
{
	int i = 0 ;
	printf("å¥‡æ•°ä½ï¼š\n");

	for(i = 30 ; i >=0 ; i-=2) //é¦–å…ˆï¼Œä»ä½ä½åˆ°é«˜ä½ï¼å…¶æ¬¡æ³¨æ„å…±æœ‰æ•´æ•°çš„äºŒè¿›åˆ¶æ•°å…±æœ‰32ä½ï¼Œi=30ä»£è¡¨çš„æ˜¯ç¬¬31ä½âœ¨
	{
		printf("%d ",((a>>i)&1)); //æŒ‰ä½ä¸
	}
	printf("\n");
	printf("å¶æ•°ä½ï¼š\n");
	for(i = 31 ; i>=1 ; i-=2) //i=31ä»£è¡¨ç¬¬32ä½
	{
		printf("%d ",((a>>i)&1));
	}
	printf("\n");
}
int main()
{
	int a  = 0 ;
	scanf("%d",&a);
	print(a);
	return 0 ;
}


//ç”¨å‡½æ•°å®ç°?*?ä¹˜æ³•å£è¯€è¡¨
void print_table(int a)
{
	int i = 0 ;
	for(i = 1; i <= a ; i++) //è¡Œ
	{
		int j = 0;
		for(j = 1 ; j <= i; j++) //åˆ—
		{
			printf("%d*%d=%-3d",i,j,i*j);
		}
		printf("\n");
	}
}
int main()
{
	int a = 0 ;
	scanf("%d",&a);
	print_table(a);
	return 0 ;
}


//é€’å½’çš„æ–¹æ³•å®ç°å­—ç¬¦ä¸²é€†åº
int my_strlen(char arr[])
{
	int i = 0 ;
	int count = 0;
	while(arr[i] != '\0')
	{
		i ++ ; //i++ä¸èƒ½å¿˜
		count ++;
	}
	return count ;
}

//é€’å½’âœ¨âœ¨âœ¨
void reverse_string(char arr[])
{
	char tmp = arr[0] ; //æŠŠaæ‹¿å‡ºæ¥
	int length = my_strlen(arr);
	arr[0] = arr[length-1];//å°†aæ¢æˆf
	arr[length -1] = '\0'; //å°†åŸæœ¬fçš„ä½ç½®æ¢ä¸Š\0 - ä¸ºäº†ä¸‹ä¸€æ­¥çš„é€’å½’é€†åº - 

	//é€’å½’å¿…é¡»è¦æœ‰é™åˆ¶æ¡ä»¶ï¼ï¼ï¼
	//if(tmp != 0) //è¿™ä¸ªæ¡ä»¶ä¸å¯¹ï¼âœ¨âœ¨
	//if(length >= 2) // ä¹Ÿä¸å¯¹ï¼è¿™é‡Œçš„lengthæ˜¯arrçš„é•¿åº¦ï¼Œè€Œä¸æ˜¯ä¸­é—´å­—ç¬¦çš„é•¿åº¦

	if(my_strlen(arr+1) >= 2) //éœ€è¦å†æ¬¡ä½¿ç”¨strlenå‡½æ•°è®¡ç®—ä¸­é—´å­—ç¬¦ä¸²é•¿åº¦ï¼ˆæœ«å°¾çš„å­—ç¬¦å·²ç»å˜æˆäº†\0ï¼‰æ˜¯å¦ >= 2æ—¶ï¼Œæ‰éœ€è¦é€†åºï¼ï¼âœ¨

	reverse_string(arr+1);//é€’å½’ - é€†åº arr+1ä»£è¡¨é¦–å…ƒç´ åœ°å€ï¼Œå¹¶ä¸”æœ€åä¸€ä½ä¹Ÿè¢«æ”¹æˆäº†\0 - å³å­—ç¬¦ä¸²å˜ä¸ºï¼š"bcde"

	arr[length -1 ] = tmp ; //æœ€åå°†\0æ¢æˆaæ”¾åœ¨æœ«å°¾âœ¨
}

int main()
{
	char arr[] = "abcdef";
	printf("%s\n",arr);
	reverse_string(arr);
	printf("%s\n",arr);
	return 0 ; 
}
