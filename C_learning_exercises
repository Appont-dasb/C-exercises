}


//æ³¨æ„èµ‹å€¼æ“ä½œç¬¦=çš„ä¼˜å…ˆçº§å¤§äº,
int main()
{
	int a ,b ,c;
	a = 5 ;
	c = ++ a ;
	b = ++c , c++ , ++a, a++ ; //bå€¼ä¸º7ï¼Œcå€¼ä¸º8
	//æ‰€ä»¥æ˜¯å…ˆb = ++cï¼Œå†è¿›è¡Œä¸‹é¢ç¨‹åº
	b += a++ + c; //aå€¼ä¸º9
	//b = 7 + 16
	printf("a = %d\nb = %d\nc = %d\n",a,b,c); //9 23 8
	return 0 ;
}


